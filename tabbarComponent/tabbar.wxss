.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 99;
    width: 100%;
    height: 140rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}
.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    position: relative;
}
.tabbar_icon{
    width: 56rpx;
    height: 56rpx;
}
.special-wrapper{
  position: absolute;
  left: 77rpx;
  top: -56rpx;
  width: 110rpx;
  height:110rpx;
  border-radius: 50%;
  /* border-top: 2rpx solid #f2f2f3; */
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  /* box-shadow: rgba(102, 102, 102, 0.12); */
  /* padding: 6rpx; */
}
/* .tabbar_nav text{
  color: #DA2C1F;
} */
.special-wrapper .tabbar_icon{
    width: 110rpx;
    height: 110rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}

.mask{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,0.4);
  z-index:100;
}

.modal-content{
  position: absolute;
  z-index: 99;
  width: 100%;
  /* height: 80%; */
  /* background-color: #fff;
  border-radius: 10rpx; */
}

.main-content{
  flex: 1;
  height: 100%;
  background: #fff;
  border-radius: 10rpx;
  overflow-y: hidden; 
}
.modal-btn-close{
  position: absolute;
  z-index: 999;
  right:0rpx;
  top:0rpx;
  width: 60rpx;
  height:80rpx;
  line-height: 80rpx;
  /* background: red; */
  vertical-align: center;
  text-align: center;
}
.modal-btn-close>image{
    width:40rpx;
    height: 40rpx;
}
.changePassword{
  position: absolute;
  left: 0;
  bottom:50rpx;
  width: 100%;
}
.modal-title{
  padding: 30rpx 40rpx;
}
.tips-list{
  padding: 10rpx 0;
}
.red{
  color:red;
}
