/* pages/projectChange/projectChange.wxss */
.container{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0;
  background: #F6F6F6;
}
.room-change-search{
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding:30rpx 40rpx;
  /* border-bottom: 1rpx solid #eee; */
  background: #fff;
}
.search-bar__box{
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
}
.room-change-search .search-img{
   position: absolute;
  top:58rpx;
  right: 60rpx;
  /* left: 60rpx; */
  width:30rpx;
  height: 30rpx;
}
.search-img>image{
  width: 100%;
  height: 100%;
}
.room-change-search input{
  width: 100%;
  padding-left:30rpx;
  /* border:1rpx solid #ccc; */
  height:88rpx;
  border-radius: 10rpx;
  /* text-align: center; */
  background: #F2F2F2
}
.list-item{
  padding:30rpx;
  background: #fff;
  border-bottom: 1rpx solid #eee;
}
.list-item:last-child{
   border-bottom:none;
}
.other{
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #888;
}
.room-type{
  flex-grow: 1
}
.message-info{
   display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border: 2rpx;
  border-radius: 50%; 
  vertical-align: middle;
  margin-right:10rpx;
}
.householder{
  background: #5656ED;
}
.dependents{
  background: #E73047;
}
.tenant{
  background: #FF8B33;
}
.otherColor{
 background: #76C945;
}
.no-message{
  padding-top: 190rpx;
}
.no-image{
  text-align: center;
}
.no-image>image{
  width: 380rpx;
  height: 358rpx;
}
.no-content{
  margin: 50rpx 0 150rpx 0;
  text-align: center;
  color: #666;
}

/*搜索*/
.info-search {
  /* position: fixed; */
  width: 92%;
  padding: 40rpx 30rpx;
  background-color: #fff;
  z-index: 999;
  border-bottom: 1rpx solid #f0f0f0;
}

.search {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 80rpx;
  background-color: whitesmoke;
  border: 1rpx solid #ddd;
  border-radius: 20rpx;
}

.search input {
  width: 80%;
  font-size: 27rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  padding-left: 30rpx;
}

.search image {
  width: 40rpx;
  height: 40rpx;
  padding-right: 20rpx;
}

