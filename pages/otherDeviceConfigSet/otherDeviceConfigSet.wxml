<!--pages/otherDeviceConfigSet/otherDeviceConfigSet.wxml-->
<view class="container">
  <view class="page-body">
    <form bindsubmit="formSubmit" bindreset="formReset">
      <view class="section" bindtap="workMode">
        <view class="section__title">
          工作模式 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.workMode}}</text>
          <input name="workMode" hidden="true" style="text-align:right;" 
             value="{{form.workMode}}" />
             <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="doorDelayTime">
        <view class="section__title">
          开锁持续时间 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.doorDelayTime}}</text>
          <input name="doorDelayTime" hidden="true" value="{{form.doorDelayTime}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="antifea">
        <view class="section__title">
          是否启动活体 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.antifea}}</text>
          <input name="antifea" hidden="true" value="{{form.antifea}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="score">
        <view class="section__title">
          识别等级（值越大越严格） <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.score}}</text>
          <input name="score" hidden="true"  value="{{form.score}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="soundSuccess">
        <view class="section__title">
          识别成功语音提示 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.soundSuccess}}</text>
          <input name="soundSuccess" hidden="true" value="{{form.soundSuccess}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="soundStranger">
        <view class="section__title">
          陌生人语音提示 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.soundStranger}}</text>
          <input name="soundStranger" hidden="true" value="{{form.soundStranger}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="light">
        <view class="section__title">
          补光灯 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.light}}</text>
          <input name="light" hidden="true" value="{{form.light}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="cardFormat">
        <view class="section__title">
          读卡格式 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.cardFormat}}</text>
          <input name="cardFormat" hidden="true" value="{{form.cardFormat}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view style="width:100%;height:60rpx;border-top:1rpx solid #eee;background:#f0f0f0;"></view>
      <view class="btn-area">
        <button class='nodisabled' bindtap="submit">提交</button>
      </view>
      <view style="width:100%;height:60rpx;border-top:1rpx solid #eee;background:#f0f0f0;"></view>
    </form>
  </view>
</view>
