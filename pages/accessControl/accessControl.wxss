.container{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #f0f0f0;
}

.list {
  display: flex;
  flex-direction: row;
  padding: 30rpx;
  border-bottom: 1rpx solid #f0f0f0;
  background:#fff;
}

.list-name {
  flex-grow: 1;
}

.list-icon>image {
  width: 30rpx;
  height: 30rpx;
}