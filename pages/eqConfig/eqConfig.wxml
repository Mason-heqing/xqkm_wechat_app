          <input name="devicedDirection" hidden="true" style="text-align:right;" 
             value="{{form.devicedDirection}}" />
             <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view> -->
      <view class="section" bindtap="authentication">
        <view class="section__title">
          认证方式 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.authentication}}</text>
          <input name="authentication" hidden="true" value="{{form.authentication}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
