      <view class="section" bindtap="faceParameter">
        <view class="section__title">
          人脸参数 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <input name="faceParameter" hidden="true" value="{{form.faceParameter}}" />
