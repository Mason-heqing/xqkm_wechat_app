      <view class="section" bindtap="volumeSet">
        <view class="section__title">
          补光灯模式 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.fillInLight}}</text>
          <input name="fillInLight" hidden="true" value="{{form.fillInLight}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <view class="section" bindtap="fillInLight">
        <view class="section__title">
          音量设置 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <text>{{form.volume}}</text>
          <input name="volume" hidden="true" value="{{form.volume}}" />
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
