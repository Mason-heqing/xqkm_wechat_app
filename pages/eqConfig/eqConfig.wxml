<!--pages/eqConfig/eqConfig.wxml-->
<view class="container">
  <view class="page-body">
    <form bindsubmit="formSubmit" bindreset="formReset">
      <view class="section" bindtap="dateSet">
        <view class="section__title">
          日期和时间 <text class="star"></text></view>
        <view class="section_name" style="text-align:right;">
          <input name="date" style="text-align:right;" hidden="true"  value="{{form.date}}" />
          <text>{{form.date}}</text>
          <image class="right_img" src='../../image/arrow.png'></image>
        </view>
      </view>
      <!-- <view class="section" bindtap="deviceDirection">
