<!--pages/addProject/addProject.wxml-->
<view class="container">
  <view class="search-project">
    <input name="name" placeholder-style="color:#999" data-name='name' bindinput='trim' maxlength="36" value="{{name}}" placeholder="请输入项目名称，最大支持36位" />
  </view>
  <view class="submit">
    <button bindtap="submit">确定</button>
  </view>
</view>
