/* pages/setMode/setMode.wxss */
.connectPage .tips{
  margin-top:20rpx;
  padding:0 30rpx;
}
.connectPage .alignCenter{
  text-align:center;
}
.connectPage .blueImg{
  display: block;
  margin:80rpx auto 50rpx;
  width:198rpx;
  height:198rpx;
}
.btn-fixed{
  position: fixed;
  bottom:20rpx;
  z-index:10;
  padding: 0 20rpx;
  height:98rpx;
  line-height:38rpx;
  background:#fff;
  box-sizing: border-box;
}
.width100{
  width:100%;
}
.loginBtn{
  background-color:rgba(0, 187, 156, 1);
  color:#fff;
  font-size:36rpx;
  height:94rpx;
  border:none !important;
  outline: none;
  border-radius:48rpx;
}
.content_item{
  padding-left:52rpx;
}
.content_item_row{
  display:flex;
  flex-direction: row;
}
.input-item{
  height: 100rpx;
  line-height: 100rpx;
  width:100%;
}
.borderLine{
  border-bottom:1rpx solid #E7E8EA;
}

.start{
  position: relative;
}
.start::before{
  position: absolute;
  content: '*';
  font-size:36rpx;
  color:rgba(247, 21, 21, 1);
  vertical-align: middle;
  right:-20rpx;
  top:3rpx;
}
.input{
  display:inline-block;
  font-size:28rpx;
  vertical-align:middle;
  margin-left:40rpx;
  width:440rpx;
  border-bottom: 1px solid #e7e8ea;
}
.input-name{
  color:rgba(51, 51, 51, 1);
  font-size:28rpx;
  margin-right: 6rpx;
  display:inline-block;
  vertical-align: middle;
}

.radio-box,.weui-cell__bd{
  display:inline-block;
  font-size:28rpx;
}
.radio-box{
  margin-right:2rpx;
}
.weui-cell__bd{
  margin-right:8rpx;
  vertical-align: middle;
}
.name-item{
  position:relative;
  z-index:99;
}
.corpListBOx{
  position: absolute;
  top: 100%;
  left: 130rpx;
  z-index:111;
  width:400rpx;
  background-color:#fff;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 5px 0px rgba(0,0,0,0.5);
  border-radius:8px;
}
.touch-scroll{
  max-height: 576rpx;
  overflow: auto;
  overscroll-behavior-y: contain;
  -webkit-overflow-scrolling: touch;
  padding:0 40rpx;
  box-sizing: border-box;
}
.list-item{
  display: flex;
  justify-content: space-between;
  border-bottom:0.5rpx solid rgba(153, 153, 153, 0.5);
  height:64rpx;
  line-height: 64rpx;
}
.list-item:last-child{
  border-bottom:0px;
}
.input-icon{
  display:inline-block;
  width:50rpx;
  height:50rpx;
  position:absolute;
  top:30rpx;
  right:40rpx;
  z-index:99;
}
.close-icon{
  right:100rpx;
}
.grap{
  margin-top: 10px;
  width:100%;
  height:10rpx;
  background:rgba(242,242,242,1);
}
.settingTips{
  color:#6E6F70;
  font-size:24rpx;
  line-height: 48rpx;
}

.roate{
  transform:rotate(180deg);
}
.body-view{
  display:inline-block;
}
.border-item{
  border-radius:19rpx;
  border:1rpx solid rgba(51,51,51,1);
  height:40rpx;
  line-height:40rpx;
  padding:1rpx 22rpx;
}
.triangle{
  width:0;
  height:0;
  border-width:10rpx;
  border-style:solid;
  border-color:rgba(51, 51, 51, 1) transparent transparent transparent;
  display:inline-block;
  vertical-align: middle;
}
.disabled{
  background-color:rgba(168, 168, 168, 0.5);
}
.switchZoom{
  zoom:0.8;
  vertical-align: middle;
}
.red{
  color:red;
}
.paddingleft52{
  padding-left:52rpx;
}